    "@canlooks/react-router": "^1.0.6",
    "@canlooks/reactive": "^4.2.1",
    "@canlooks/utils": "^2.1.0",
    "@emotion/react": "^11.14.0",
    "@types/node": "^22.10.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "react": "^19.0.0",
